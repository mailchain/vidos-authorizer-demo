{
	"name": "vidos-authorizer-demo",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/mailchain/vidos-authorizer-demo"
	},
	"scripts": {
		"dev": "vite",
		"type-check": "tsc --noEmit",
		"build": "tsc -b && vite build",
		"lint": "biome lint",
		"format": "biome check --write",

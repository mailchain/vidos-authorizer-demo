{
	"name": "vidos-authorizer-example",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"type-check": "tsc --noEmit",
		"build": "tsc -b && vite build",
		"lint": "biome lint",
		"format": "biome check --write",
		"preview": "vite preview",
		"generate-api": "openapi-typescript https://vidos.id/docs/spec/openapi/authorizer.service.yaml --output src/api/authorizer.ts"
	},
	"dependencies": {
		"openapi-fetch": "^0.15.0",
		"react-dom": "^19.2.0",
		"react": "^19.2.0"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.11",
		"@tailwindcss/vite": "^4.1.18",
		"@types/node": "^24.10.1",
		"@types/react-dom": "^19.2.3",
		"@types/react": "^19.2.5",
		"@vitejs/plugin-react": "^5.1.1",
		"globals": "^16.5.0",
		"openapi-typescript": "^7.10.1",
		"tailwindcss": "^4.1.18",
		"typescript": "~5.9.3",
		"vite": "^7.2.4"
	}
}
